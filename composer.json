{
  "name": "supplycart/snapshot",
  "description": "Capture model state over time",
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "require": {
    "php": "^7.4|^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.2",
    "orchestra/canvas": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Supplycart\\Snapshot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Supplycart\\Snapshot\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Supplycart\\Snapshot\\SnapshotServiceProvider"
      ]
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Azri Ismail",
      "email": "azri@supplycart.my"
    }
  ]
}
